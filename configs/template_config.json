{
  "session" : {
    "aws_access_key_id" : "<aws access key>",
    "aws_secret_access_key" : "<aws secret key>",
    "region_name" : "<prefered region>"
  },
  "instances" : [
    {
      "ImageId" : "<base ami>",
      "MinCount" : 1,
      "MaxCount" : 1,
      "KeyName" : "<key pair>",
      "SecurityGroups" : [
        "<security group>"
      ],
      "InstanceType" : "t2.micro"
    }
  ],
  "ssh" : {
    "key_path" : "<path to .ssh dir>"
  },
  "ftp" : {
    "config_path" : "<path to repo>/aws-elasticsearch-deploy/configs/",
    "configs" : [
      "nginx-elasticsearch-proxy.conf",
      "openssl-prompt.conf",
      "self-signed.conf",
      "ssl-params.conf"
    ],
    "script_path" : "<path to repo>/aws-elasticsearch-deploy/scripts/",
    "setup_script" : "elasticsearch_setup.sh"
  },
  "user_auth" : [
    {
      "username" : "<your user>",
      "password" : "<your password>"
    }
  ]
}

